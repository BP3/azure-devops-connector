{
  "$schema": "https://unpkg.com/@camunda/zeebe-element-templates-json-schema/resources/schema.json",
  "name": "Azure Devops Connector",
  "id": "c683e70f-0a0c-4977-8695-6664d987cb39",
  "version": 1702997482342,
  "description": "Get, create tasks and issues for your team in azure devops",
  "icon": {
    "contents": "data:image/png;base64,"
  },
  "category": {
    "id": "connectors",
    "name": "Connectors"
  },
  "appliesTo": [
    "bpmn:Task"
  ],
  "elementType": {
    "value": "bpmn:ServiceTask"
  },
  "groups": [
    {
      "id": "authentication",
      "label": "Authentication"
    },
    {
      "id": "configuration",
      "label": "Configuration"
    },
    {
      "id": "operation",
      "label": "Operation"
    },
    {
      "id": "payload",
      "label": "Payload"
    },
    {
      "id": "input",
      "label": "Input"
    },
    {
      "id": "output",
      "label": "Output"
    },
    {
      "id": "errors",
      "label": "Error handling"
    }
  ],
  "properties": [
    {
      "type": "Hidden",
      "value": "io.camunda:http-json:1",
      "binding": {
        "type": "zeebe:taskDefinition:type"
      }
    },
    {
      "type": "Hidden",
      "label": "Type",
      "id": "authenticationType",
      "group": "authentication",
      "value": "basic",
      "binding": {
        "type": "zeebe:input",
        "name": "authentication.type"
      }
    },
    {
      "type": "Hidden",
      "label": "UserName",
      "id": "username",
      "group": "authentication",
      "value": "xyz",
      "binding": {
        "type": "zeebe:input",
        "name": "authentication.username"
      }
    },
    {
      "type": "String",
      "label": "Personal Access Token",
      "id": "password",
      "group": "authentication",
      "description": "Personal Access Token",
      "feel": "optional",
      "value": "",
      "binding": {
        "type": "zeebe:input",
        "name": "authentication.password"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "group": "configuration",
      "type": "Hidden",
      "value": "={\"Content-Type\":\"application/json-patch+json\"}",
      "binding": {
        "type": "zeebe:input",
        "name": "headers"
      },
      "constraints": {
        "notEmpty": true
      },
      "condition": {
        "property": "operationType",
        "oneOf": [
          "createWorkItem"
        ]
      }
    },
    {
      "label": "Azure Devops Organization",
      "description": "Name of the devops organization",
      "group": "configuration",
      "type": "String",
      "feel": "optional",
      "value": "",
      "binding": {
        "type": "zeebe:input",
        "name": "organization"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Project",
      "description": "Project ID",
      "group": "configuration",
      "type": "String",
      "feel": "optional",
      "value": "",
      "binding": {
        "type": "zeebe:input",
        "name": "project"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Operation group",
      "id": "operationGroup",
      "group": "operation",
      "description": "Choose operation group",
      "value": "issue",
      "type": "Dropdown",
      "choices": [
        {
          "name": "Issue",
          "value": "issue"
        },
        {
          "name": "Task",
          "value": "task"
        }
      ],
      "binding": {
        "type": "zeebe:input",
        "name": "operationGroup"
      }
    },
    {
      "label": "Operation type",
      "id": "operationType",
      "group": "operation",
      "type": "Dropdown",
      "value": "getWorkItem",
      "choices": [
        {
          "name": "Create Issue",
          "value": "createWorkItem"
        },
        {
          "name": "Get Issue",
          "value": "getWorkItem"
        }
      ],
      "binding": {
        "type": "zeebe:input",
        "name": "operationType"
      },
      "condition": {
        "property": "operationGroup",
        "oneOf": [
          "issue"
        ]
      }
    },
    {
      "label": "Operation type",
      "id": "operationType",
      "group": "operation",
      "type": "Dropdown",
      "value": "getWorkItem",
      "choices": [
        {
          "name": "Create Task",
          "value": "createWorkItem"
        },
        {
          "name": "Get Task",
          "value": "getWorkItem"
        }
      ],
      "binding": {
        "type": "zeebe:input",
        "name": "operationType"
      },
      "condition": {
        "property": "operationGroup",
        "oneOf": [
          "task"
        ]
      }
    },
    {
      "id": "methodPost",
      "group": "configuration",
      "type": "Hidden",
      "value": "post",
      "binding": {
        "type": "zeebe:input",
        "name": "method"
      },
      "condition": {
        "property": "operationType",
        "oneOf": [
          "createWorkItem"
        ]
      }
    },
    {
      "id": "methodGet",
      "group": "configuration",
      "type": "Hidden",
      "value": "get",
      "binding": {
        "type": "zeebe:input",
        "name": "method"
      },
      "condition": {
        "property": "operationType",
        "oneOf": [
          "getWorkItem"
        ]
      }
    },
    {
      "label": "Work Item Id",
      "description": "Id of the Work Item",
      "group": "input",
      "type": "String",
      "feel": "optional",
      "value": "",
      "binding": {
        "type": "zeebe:input",
        "name": "workItemId"
      },
      "constraints": {
        "notEmpty": true
      },
      "condition": {
        "property": "operationType",
        "oneOf": [
          "getWorkItem"
        ]
      }
    },
    {
      "label": "Title",
      "description": "Title of the Work Item",
      "group": "input",
      "type": "String",
      "feel": "optional",
      "binding": {
        "type": "zeebe:input",
        "name": "title"
      },
      "constraints": {
        "notEmpty": true
      },
      "condition": {
        "property": "operationType",
        "oneOf": [
          "createWorkItem"
        ]
      }
    },
    {
      "label": "Result variable",
      "description": "Name of variable to store the response in. Details in the <a href=\"https://docs.camunda.io/docs/components/connectors/use-connectors/#result-variable\" target=\"_blank\">documentation</a>",
      "group": "output",
      "feel": "optional",
      "type": "String",
      "binding": {
        "type": "zeebe:taskHeader",
        "key": "resultVariable"
      }
    },
    {
      "label": "Result expression",
      "id": "resultExpression",
      "description": "Expression to map the response into process variables. Details in the <a href=\"https://docs.camunda.io/docs/components/connectors/use-connectors/#result-expression\" target=\"_blank\">documentation</a>",
      "group": "output",
      "type": "String",
      "feel": "required",
      "value": "={workItem: response.body}",
      "binding": {
        "type": "zeebe:taskHeader",
        "key": "resultExpression"
      }
    },
    {
      "label": "Connection timeout",
      "description": "Sets the timeout in seconds to establish a connection or 0 for an infinite timeout",
      "group": "errors",
      "type": "String",
      "value": "20",
      "binding": {
        "type": "zeebe:input",
        "name": "connectionTimeoutInSeconds"
      },
      "optional": true,
      "feel": "optional",
      "constraints": {
        "notEmpty": false,
        "pattern": {
          "value": "^(=.+|[0-9]+|\\{\\{secrets\\..+\\}\\})$",
          "message": "Must be a timeout in seconds (default value is 20 seconds) or a FEEL expression"
        }
      }
    },
    {
      "type": "Hidden",
      "value": "=[{ \"op\": \"add\", \"path\": \"/fields/System.Title\", \"from\": null, \"value\":   string(title)  }]",
      "binding": {
        "name": "body",
        "type": "zeebe:input"
      },
      "condition": {
        "property": "operationType",
        "oneOf": [
          "createWorkItem"
        ]
      }
    },
    {
      "type": "Hidden",
      "value": "=\"https://dev.azure.com/\"",
      "binding": {
        "type": "zeebe:input",
        "name": "baseUrl"
      }
    },
    {
      "type": "Hidden",
      "value": "=\"/_apis/wit/workitems/\"",
      "binding": {
        "type": "zeebe:input",
        "name": "workItemUrlSuffix"
      }
    },
    {
      "type": "Hidden",
      "value": "=\"?api-version=7.1-preview.3\"",
      "binding": {
        "type": "zeebe:input",
        "name": "getWorkItemQueryString"
      }
    },
    {
      "type": "Hidden",
      "value": "=baseUrl + organization  +\"/\" + project + workItemUrlSuffix + string(workItemId) + getWorkItemQueryString",
      "binding": {
        "type": "zeebe:input",
        "name": "url"
      },
      "condition": {
        "property": "operationType",
        "oneOf": [
          "getWorkItem"
        ]
      }
    },
    {
      "type": "Hidden",
      "value": "=baseUrl + organization  +\"/\" + project + workItemUrlSuffix +\"$\" + operationGroup + getWorkItemQueryString",
      "binding": {
        "type": "zeebe:input",
        "name": "url"
      },
      "condition": {
        "property": "operationType",
        "oneOf": [
          "createWorkItem"
        ]
      }
    },
    {
      "label": "Error expression",
      "description": "Expression to handle errors. Details in the <a href=\"https://docs.camunda.io/docs/components/connectors/use-connectors/#bpmn-errors\" target=\"_blank\">documentation</a>",
      "group": "errors",
      "type": "Text",
      "feel": "required",
      "binding": {
        "type": "zeebe:taskHeader",
        "key": "errorExpression"
      }
    }
  ]
}